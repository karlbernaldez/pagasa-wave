{
  "version": 1,
  "type": "rasterarray",
  "minzoom": 0,
  "maxzoom": 3,
  "sources": [{"uri": "mapbox://tileset-source/votewave/ECMWF"}],
  "layers": {
    "10m_wind": {
      "tilesize": 512,
      "resampling": "bilinear",
      "buffer": 1,
      "source_rules": {
        "filter": [
          ["all", ["==", ["get", "GRIB_COMMENT"], "u-component of wind [m/s]"]],
          ["all", ["==", ["get", "GRIB_COMMENT"], "v-component of wind [m/s]"]]
        ],
        "name": ["to-number", ["get", "GRIB_VALID_TIME"]],
        "order": "asc",
        "sort_key": ["to-number", ["get", "GRIB_VALID_TIME"]]
      }
    }
  }
}